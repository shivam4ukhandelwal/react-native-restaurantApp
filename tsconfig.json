{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"],
      "@hooks/*": ["hooks/*"],
      "@store/*": ["store/*"],
      "@types/*": ["types/*"],
      "@services/*": ["services/*"],
      "@routes/*": ["routes/*"],
      "@themes/*": ["themes/*"]
    }
  },
  "include": ["src"]
}